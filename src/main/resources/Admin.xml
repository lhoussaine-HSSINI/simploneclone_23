<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="MVC.Model.Admin">
        <table name="admin" schema="simplone" catalog=""/>
        <attributes>
            <id name="idAdmin">
                <column name="id_admin" precision="11"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
